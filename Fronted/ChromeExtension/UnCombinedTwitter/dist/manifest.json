{
  "name": "RepFeed",
  "description": "A Vue.js web extension",
  "version": "2.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "RepFeed",
    "default_icon": {
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://platform.twitter.com 'unsafe-eval'; object-src 'self'"
}